//WAP to print LCM of Given two Numbers.......
import java.util.Scanner;
class LCMofTwoNumUsingFor 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter Two Numbers One by One...");
		int n1=sc.nextInt();
		int n2=sc.nextInt();
		for (int i=1; i>0; i++)
		{
			if ((n1*i)%n2==0)
			{
				System.out.println("lcm is :"+n1*i);
				break;
			}
		}
	}
}

