//WAP to print count of Faactor's
import java.util.Scanner;
class CountOfFactor 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter a Number:");
		int n=sc.nextInt();
		int count=0;
		for(int i=1; i<=n; i++)
		{
			if(n%i==0)
			{
			count++;
			}
		}
		System.out.println("Count of factor is :"+count+" "+sum);
			
	}
}

