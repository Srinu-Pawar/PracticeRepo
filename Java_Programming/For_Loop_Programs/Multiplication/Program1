

import java.util.Scanner;
class MulTableGivenRange 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter Min Number of a Table : ");
		int min=sc.nextInt();
		System.out.print("Enter Max Number of a Table : ");
		int max=sc.nextInt();
		for(int i=min; i<=max; i++)
		{
			for(int j=1; j<=10; j++)
			{
				System.out.println(i+" X "+j+" = "+i*j);

			}
			System.out.println("-----------------------");
		}
	}
}
