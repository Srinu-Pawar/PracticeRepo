//WAP to print HCF of Two given Numbers ......
import java.util.Scanner;
class HCFofGivenTwoNum 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter Two Numbers one by one");
		int n1=sc.nextInt();
		int n2=sc.nextInt();
		int hcf=0;
		for (int i=1; i<=n1 &&i<=n1; i++)
		{
			if (n1%i==0 && n2%i==0)
			{
				hcf=i;
			}
		}
		System.out.println("HCF of two number is :"+hcf);
	}
}

