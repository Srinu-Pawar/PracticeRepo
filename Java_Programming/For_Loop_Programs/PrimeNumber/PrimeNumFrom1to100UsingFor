//WAP to print Prime Number from 1 to 100 with sln(Serial) Using For loop
class PrimeNumFrom1to100UsingFor 
{
	public static void main(String[] args) 
	{
		System.out.println("Prime Numbers from 1 to 100");
		for (int i=1; i<=100 ; i++)
		{
			int count=0;
			for (int j=1; j<=i; j++)
			{
				if (i%j==0)
				{
					count++;
				}
			}
			if (count==2)
			System.out.println(i);	
		}	
	}
}
