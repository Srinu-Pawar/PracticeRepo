//WAP to print Prime Number from 1 to 100 with sln(Serial) using while loop
class PrimeNumFrom1to100UsingWhile 
{
	public static void main(String[] args) 
	{
		System.out.println("Prime Numbers from 1 to 100");
		int i=1,sln=1;
		while (i<=100)
		{
			int count=0;
			int j=1;
			while ( j<=i)
			{
				if (i%j==0)
				{
					count++;
				}
				j++;
			}
			if (count==2)
			{
			System.out.println(sln+")"+i);	
			sln++;
			}
		i++;
		}	
	}
}



